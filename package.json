{
  "name": "sebastienbarbier",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "install": "npm run build:dynamic",
    "start": "node dist/server",
    "build": "ng build",
    "build:client-and-server-bundles": "ng build sebastienbarbier_en --configuration production && ng build sebastienbarbier_fr --configuration production && ng run sebastienbarbier_en:server",
    "build:static": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:static",
    "generate:static": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress && cd static/en && copy ./* ../../dist/en && cd ../fr && copy ./* ../../dist/fr",
    "serve:static": "cd dist/en && http-server",
    "test": "ng test",
    "lint": "ng lint",
    "i18n": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,fr}.json --clean --sort --format namespaced-json -m _",
    "e2e": "ng e2e",
    "fetch:socials": "npm run instagram && npm run nomadlist",
    "instagram": "node scripts/instagram.js",
    "nomadlist": "node scripts/nomadlist.js",
    "dev:ssr": "ng run sebastienbarbier_en:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build && ng run sebastienbarbier_en:server",
    "prerender": "ng run sebastienbarbier_en:prerender"
  },
  "engines": {
    "node": "~14"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.4",
    "@angular/common": "^12.1.4",
    "@angular/compiler": "^12.1.4",
    "@angular/core": "^12.1.4",
    "@angular/forms": "^12.1.4",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^12.1.4",
    "@angular/platform-browser-dynamic": "^12.1.4",
    "@angular/platform-server": "^12.1.4",
    "@angular/router": "^12.1.4",
    "@nguniversal/express-engine": "^12.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@sentry/browser": "^6.10.0",
    "aos": "^2.3.4",
    "core-js": "^3.16.0",
    "country-flag-emoji": "^1.0.3",
    "express": "^4.15.2",
    "express-sslify": "^1.2.0",
    "geo-tz": "^6.0.1",
    "mapbox-gl": "^2.4.0",
    "rxjs": "^7.3.0",
    "tslib": "^2.3.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.4",
    "@angular/cli": "^12.1.4",
    "@angular/compiler-cli": "^12.1.4",
    "@angular/language-service": "^12.1.4",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@nguniversal/builders": "^12.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.8.2",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^16.4.10",
    "codelyzer": "^6.0.2",
    "copy": "^0.3.2",
    "dotenv": "^10.0.0",
    "html-webpack-plugin": "^5.3.2",
    "http-server": "^0.12.3",
    "jasmine-core": "^3.8.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "node-fetch": "^2.6.1",
    "protractor": "^7.0.0",
    "reflect-metadata": "^0.1.13",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "webpack": "^5.47.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
